    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictNullChecks": true,
    "types": [
      "vite-plugin-pwa/info"
    ],
    "paths": {
      "@/*": ["src/*"],
    },
    "lib": ["ESNext", "DOM", "WebWorker"],
  }
}